// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GeneratedCoin {
  id      Int    @id @default(autoincrement())
  digest  String @unique
  tokenID Int
  x       Int
  y       Int
  creator String
}

model GorblinCoin {
  id      Int    @id @default(autoincrement())
  digest  String @unique
  tokenID Int
  x       Int
  y       Int
  creator String
  claimer String
}
